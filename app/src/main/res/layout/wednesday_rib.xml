<?xml version="1.0" encoding="utf-8"?>
<com.mazzeom.app.armstrong.root.main.daily_tab.weekday.wednesday.WednesdayView
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:orientation="horizontal">

	<LinearLayout
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:orientation="vertical" >

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="60dp"
			android:text="Pull up"
			android:textColor="@color/category2"
			android:textSize="17sp"
			android:paddingLeft="24dp"
			android:textFontWeight="700"
			android:gravity="center_vertical"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="60dp"
			android:text="Chin up"
			android:textColor="@color/category2"
			android:textSize="17sp"
			android:paddingLeft="24dp"
			android:textFontWeight="700"
			android:gravity="center_vertical"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="60dp"
			android:text="Wide pull up"
			android:textColor="@color/category2"
			android:textSize="17sp"
			android:paddingLeft="24dp"
			android:textFontWeight="700"
			android:gravity="center_vertical"/>
	</LinearLayout>

	<GridLayout
		android:id="@+id/pullUpGridLayout"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:paddingLeft="20dp"
		android:columnCount="3"
		android:orientation="horizontal" />

</com.mazzeom.app.armstrong.root.main.daily_tab.weekday.wednesday.WednesdayView>